{
  "name": "logerator",
  "version": "1.0.0",
  "description": "A typescript decorator to log class method calls",
  "main": "lib/index.js",
  "types": "lib/index",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "test": "npm run build && node_modules/.bin/jest"
  },
  "keywords": [
    "typescript",
    "log",
    "logger",
    "decorator"
  ],
  "author": "Chris Santos",
  "repository": "chrispaulsantos/logerator",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
